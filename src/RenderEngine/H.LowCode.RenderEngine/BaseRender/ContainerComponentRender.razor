@namespace H.LowCode.RenderEngine

@foreach(var component in ContainerComponent.Childrens)
{
    <ComponentRender Component="@component"></ComponentRender>
}

@code {
    [CascadingParameter(Name = "appId")]
    public string AppId { get; set; }

    [CascadingParameter(Name = "pageId")]
    public string PageId { get; set; }

    [Parameter]
    public ComponentSchema ContainerComponent { get; set; }

    protected override async Task OnInitializedAsync()
    {

        await base.OnInitializedAsync();
    }
}